@echo off

rem Assumption: This batch file is installed to ${INSTALL_BINDIR} and the Launcher is installed to ${INSTALL_CODE_DIR}
set "WorkingFolder=%~dp0"

rem Hint at local installation under ${INSTALL_BINDIR}, required since ${PROJECT_NAME} is shared by both Debug and Release builds
set "INTEGRATIONBUS_BINPATH=%WorkingFolder%"
set "INTEGRATIONBUS_LIBPATH=%WorkingFolder%..\lib"

python "%WorkingFolder%..\..\${PROJECT_NAME}\IbLauncher.py" %*
