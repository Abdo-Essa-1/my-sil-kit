{
  "ConfigName": "simple app",
  "ConfigVersion": "0.0.0",
  "Description": "My simple VIB application",
  "MiddlewareConfig": {
    "ActiveMiddleware": "VAsio",
  },
  "SimulationSetup": {
    "Links": [
      {
        "Name": "DataService"
        "Endpoints": [ "PublisherParticipant/DataService", "SubscriberParticipant/DataService" ],
      }
    ],
    "Participants": [
      {
        "Name": "PublisherParticipant",
        "ParticipantController": {
          "SyncType": "DiscreteTime"
        },
        "GenericPublishers": [
          {
            "DefinitionUri": "",
            "Name": "DataService",
            "Protocol": ""
          }
        ],
        "IsSyncMaster": true,
        "Logger": {
          "Sinks": [
            {
              "Type": "Stdout"
            }
          ]
        }
      },
      {
        "Name": "SubscriberParticipant",
        "ParticipantController": { "SyncType": "DiscreteTime" }
        "GenericSubscribers": [ "DataService" ],
        "Logger": {
          "Sinks": [
            {
              "Type": "Stdout"
            }
          ]
        }
      },
      {
        "Name": "SystemController"
      }
    ],
    "TimeSync": {
      "TickPeriodNs": 10000000
    }
  }
}
